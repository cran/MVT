citHeader("To cite MVT package in publications use:")

citEntry(entry = "Manual",
  title = "Estimation and testing for the multivariate t-distribution",
  author = personList(as.person("F. Osorio")),
  year = "2023",
  note = "R package version 0.3-8",
  url = "http://mvt.mat.utfsm.cl",
  textVersion =
  paste("Osorio, F. (2023). Estimation and testing for the multivariate t-distribution.",
  "R package version 0.3-8.", "URL: http://mvt.mat.utfsm.cl")
)
